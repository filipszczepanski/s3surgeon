  "repository": {
    "type": "git",
    "url": "git+https://github.com/neoskop/s3surgeon.git"
  },
  "author": "Arne Diekmann <diekmann@neoskop.de>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/neoskop/s3surgeon/issues"
  },
  "homepage": "https://github.com/neoskop/s3surgeon#readme",
  "dependencies": {
    "aws-sdk": "2.710.0",
    "chalk": "4.1.0",
    "commander": "5.1.0",
    "mime-types": "2.1.27",
    "p-limit": "3.0.1"
  },
  "devDependencies": {
    "@types/jest": "26.0.4",
    "@types/mime-types": "2.1.0",
    "@types/node": "14.0.18",
    "jest": "26.1.0",
    "nodemon": "2.0.4",
    "ts-jest": "26.1.1",
    "ts-node": "8.10.2",
    "typescript": "3.9.6"
  },
  "publishConfig": {
    "access": "public"
  }
